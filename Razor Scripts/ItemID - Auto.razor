skill 'ItemID'
waitfortarget 1000

overhead "::: Searching For Items :::"
overhead "::: Please Be Patient :::"   
overhead "::: This May Take Seconds :::"
foreach x in ItemTypes
if findtype x true
    targettype x true    
    overhead '::: Item Found :::'
    overhead x
    overhead "::: ::: ::: ::: ::: :::"
        pause 1000
        if insysmsg 'You are not certain.'
            pause 1000
            replay
        endif
        if insysmsg 'magical'
            overhead '::: Lifting :::'
            lift lasttarget 1
            //BACKPACK
            pause 1000
            overhead '::: Dropping in Bag :::'
            drop 'IDBag' -1 -1 0
            //drop 'IDbag' -1 -1 0
            pause 1000
            replay
         endif
         if insysmsg 'too far'
             drop Backpack -1 -1 0
             pause 1000
             replay
         endif
         if insysmsg 'appraise'
            overhead 'lifting'
            lift lasttarget 1
            //BACKPACK
            pause 1000
            overhead 'dropping'
            drop 'IDBag' -1 -1 0
            //drop 'IDbag' -1 -1 0
            pause 1000
            replay
        endif            
    endif
endfor
hotkey "Cancel Current Target"
